.liushi_container {
  width: 100vw;
  /* padding: 0 20rpx; */
  background-color: #F4F5F9;
}

/* 瀑布流布局 */
.picture {
  column-gap: 5rpx;
  column-count: 2;
  margin: 20rpx 0;
}

.item {
  width: calc(100% - 30rpx);
  break-inside: avoid;
  /* box-shadow: 0 5px 15px 0 rgba(0, 0, 0, 0.1); */
  border-radius: 10rpx;
  margin: 0 15rpx;
}

.item .image {
  width: 100%;
  border-radius: 15rpx;
}

.null-wrap {
  height: 30rpx;
}

.font-mini {
  font-size: 10px;
}

.gray {
  color: gray;
}

.delete {
  width: 10px;
  height: 10px;
  float: right;
}
.goTop{
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  right: 10rpx;
  bottom: 55rpx;
  border-radius: 50%;
  z-index: 999;
  background-color:rgba(255, 255, 2555, .6)
}