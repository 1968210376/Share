<view class="liushi_container">
  <view style="float: left;">
    <view class="picture">
      <block wx:for="{{list}}" wx:key="index" wx:if="{{index%2==0}}">
        <view class="item" data-id="{{index}}" bindtap="wupin_detail" style="background-color: #fff;">
          <view wx:if="{{item.target.images[0]}}">
            <image class="image" lazy-load mode="widthFix" src="{{item.target.images[0]}}" />
          </view>
          <view style="font-size: 14px;padding-left: 10rpx;">{{ item.target.title }}</view>
          <view wx:if="{{!item.target.images}}" style="height: 50rpx;">
            <!-- 11 -->
          </view>
          <view style="display: flex; justify-content: space-between; align-items: center;">
            <view style="display: flex; align-items: center;">
              <view>
                <image class="icon" src="{{item.target.avatar_url}}" mode="widthFix" />
              </view>
              <text class="font-mini">{{item.target.nick_name}}</text>
            </view>
            <view wx:if="{{item.target.pirce}}" class="red price right">
              ￥{{item.target.pirce}}
            </view>
          </view>
        </view>
        <view class="null-wrap"></view>
      </block>
    </view>
    <view class="picture">
      <block wx:for="{{list}}" wx:key="index" wx:if="{{index%2==1}}">
        <view class="item" data-id="{{index}}" bindtap="wupin_detail" style="background-color: #fff;">
          <view wx:if="{{item.target.images[0]}}">
            <image class="image" lazy-load mode="widthFix" src="{{item.target.images[0]}}" />
          </view>
          <view style="font-size: 14px;padding-left: 10rpx;">{{ item.target.title }}</view>
          <view wx:if="{{!item.target.images}}" style="height: 50rpx;">
            <!-- 11 -->
          </view>
          <view style="display: flex; justify-content: space-between; align-items: center;">
            <view style="display: flex; align-items: center;">
              <view>
                <image class="icon" src="{{item.target.avatar_url}}" mode="widthFix" />
              </view>
              <text class="font-mini">{{item.target.nick_name}}</text>
            </view>
            <view wx:if="{{item.target.pirce}}" class="red price right">
              ￥{{item.target.pirce}}
            </view>
          </view>
        </view>
        <view class="null-wrap"></view>
      </block>
    </view>
  </view>

  <!-- 空列表提示 -->
  <view wx:if="{{ list.length == 0 }}" class="center">
    <image class="data-img" lazy-load mode="aspectFill" src="/images/no_result.png" />
    <view>还没有数据哦~</view>
  </view>
